        <Canvas>
            <TextBox x:Name="TextboxStepArrayY" HorizontalAlignment="Left" Height="29" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="14" TextChanged="TextboxStepArrayY_TextChanged" PreviewTextInput="TextboxStepArrayY_PreviewTextInput" Canvas.Left="137" Canvas.Top="7" Cursor="Arrow"/>
