        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Курсовик_1"
        mc:Ignorable="d"
        Title="Формирование массива Y" Height="124" Width="293" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
